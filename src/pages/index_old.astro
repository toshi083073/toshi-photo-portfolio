---
const BASE = (import.meta.env.BASE_URL || "/").replace(/\/?$/, "/");
Astro.redirect(`${BASE}landingpageTest`);
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="refresh" content="1;url={`${BASE}landingpageTest`}" />
    <title>Redirecting…</title>
  </head>
  <body>
    <p>Redirecting to <a href={`${BASE}landingpageTest`}>{`${BASE}landingpageTest`}</a> …</p>
    <script>
      (function(){
        try{
          var base=(import.meta&&import.meta.env&&import.meta.env.BASE_URL)||"/";
          if(!base.endsWith("/")) base+="/";
          location.replace(base+"landingpageTest");
        }catch(e){}
      })();
    </script>
  </body>
</html>
