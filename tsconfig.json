{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "baseUrl": ".",
    "types": ["astro/client", "node"],
    "jsx": "react-jsx",
    "allowJs": true
  },
  "include": [".astro/types.d.ts", "src", "src/env.d.ts"],
  "exclude": ["dist"]
}
